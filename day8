class Bank{
    constructor(initialamount=0){
        this._balance=initialamount;
    }
    set _balance(amountdeposit){
        if(amountdeposit>0){
            this.balance += amountdeposit;
            console.log(_balance)

        }
    }
}
let add=new Bank(10);
console.log(add);
add.balance=300;
console.log(add.balance);


